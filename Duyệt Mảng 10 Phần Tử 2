import java.util.Scanner;

public class KhaiBaoMangSoNguyen10PhanTu {
   



    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        int[] arrNumber = new int[10];
        for (int i = 0; i < 10; i++) {
            System.out.println("Nhap so" + i);
            arrNumber[i] = input.next();

        }
        for (int i = 0; i < 10; i++){
            System.out.println(arrNumber[1]);

    }}}









